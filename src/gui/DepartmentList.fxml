<VBox prefHeight="410.0" prefWidth="731.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.DepartmentListController">
   <children>
      <Label text="Department Registration">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <padding>
            <Insets left="5.0" top="5.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
      <ToolBar prefHeight="70.0" prefWidth="731.0">
        <items>
          <Button fx:id="btnNew" mnemonicParsing="false" onAction="#onBtnNewAction" text="New" />
        </items>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="20.0" />
         </padding>
      </ToolBar>
      <TableView fx:id="tableViewDepartment" prefHeight="149.0" prefWidth="679.0">
        <columns>
          <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="tableColumnName" minWidth="100.0" prefWidth="328.8000259399414" text="Name" />
            <TableColumn fx:id="tableColumnEdit" prefWidth="66.4000244140625" />
            <TableColumn fx:id="tableColumnRemove" prefWidth="66.39993286132812" />
        </columns>
         <VBox.margin>
